<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.fish56.mybatisplusimpl.mapper">
    <select id="selectByAge" resultType="com.github.fish56.mybatisplusimpl.domain.User">
        select * from User where age > #{age};
    </select>
</mapper>